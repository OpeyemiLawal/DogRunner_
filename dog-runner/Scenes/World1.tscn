// NOTE: Shader automatically converted from Godot Engine 4.5.stable's PanoramaSkyMaterial.

shader_type sky;

uniform sampler2D source_panorama : filter_linear, source_color, hint_default_black;
uniform float exposure : hint_range(0, 128) = 1.0;
uniform float speed = 0.002;

void sky() {
	vec2 skyCords = vec2(SKY_COORDS.x + TIME * speed, SKY_COORDS.y );
	COLOR = texture(source_panorama, skyCords).rgb * exposure;
}
"

